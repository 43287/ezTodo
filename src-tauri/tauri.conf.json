{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "eztodo",
  "version": "0.1.0",
  "identifier": "com.prism.eztodo",
  "build": {
    "beforeDevCommand": "pnpm -C otherP dev",
    "devUrl": "http://localhost:3001",
    "beforeBuildCommand": "pnpm -C otherP build",
    "frontendDist": "../otherP/out"
  },
  "app": {
    "windows": [
      {
        "title": "eztodo",
        "width": 1100,
        "height": 900,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ]
  }
}
